{
  "name": "premium-store-bot",
  "version": "1.0.0",
  "description": "Telegram bot assistant for premium account store selling GitHub Copilot, GitHub Student, and cloud panel accounts",
  "main": "src/bot.js",
  "scripts": {
    "start": "node src/bot.js",
    "server": "node server.js",
    "test": "jest",
    "test:unit": "jest tests/unit --passWithNoTests",
    "test:integration": "jest tests/integration",
    "test:contract": "jest tests/contract",
    "test:watch": "jest --watch",
    "migrate": "knex migrate:latest",
    "migrate:rollback": "knex migrate:rollback",
    "migrate:make": "knex migrate:make",
    "seed": "knex seed:run",
    "backup": "node scripts/backup.js",
    "recovery": "node scripts/recovery.js",
    "lint": "eslint src tests",
    "lint:fix": "eslint src tests --fix",
    "format": "prettier --write \"src/**/*.js\" \"tests/**/*.js\"",
    "format:check": "prettier --check \"src/**/*.js\" \"tests/**/*.js\"",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:contract",
    "pre-commit": "npm run lint && npm run format:check && npm run test:all",
    "prepare": "husky"
  },
  "keywords": [
    "telegram",
    "bot",
    "premium-accounts",
    "qris",
    "payment"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "telegraf": "^4.15.0",
    "knex": "^3.0.1",
    "pg": "^8.11.3",
    "mysql2": "^3.6.5",
    "ioredis": "^5.3.2",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "duitku": "^0.0.7",
    "dotenv": "^16.3.1",
    "node-cron": "^3.0.3"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "@types/node": "^20.11.0",
    "husky": "^9.1.7",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  }
}
